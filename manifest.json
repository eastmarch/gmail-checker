{
   "name": "Gmail Checker",
   "version": "0.8",
   "manifest_version": 2,
   "short_name": "gmail-checker",
   "description": "Shows badge notification for unread emails.",
   "homepage_url": "https://github.com/eastmarch/gmail-checker",
   "permissions": [ "*://mail.google.com/*", "storage", "alarms", "contextMenus" ],
   "key": "",
   "background": {
      "persistent": false,
      "scripts": [ "lib/config.js", "lib/chrome.js", "lib/common.js" ]
   },
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
   },
   "browser_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png",
         "48": "data/icons/48.png",
         "64": "data/icons/64.png"
      },
      "default_title": "Gmail Checker"
   },
   "options_ui": {
      "chrome_style": true,
      "open_in_tab": false,
      "page": "data/options/options.html"
   },
   "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+G"
        }
      }
    }
}
